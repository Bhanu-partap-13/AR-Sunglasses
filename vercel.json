{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/models/(.*\\.glb)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "model/gltf-binary"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
